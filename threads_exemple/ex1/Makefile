CC	=	gcc

RM	=	rm -f

SRC	=	ex1.c

NAME	=	test01_pthread

all	:	$(NAME)

$(NAME)	:
		$(CC) -o $(NAME) $(SRC) -lpthread

fclean	:
		$(RM) $(NAME)

re	:	fclean all

.PHONY	:	all fclean re
